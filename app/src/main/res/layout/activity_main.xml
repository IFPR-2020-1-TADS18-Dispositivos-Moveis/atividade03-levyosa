<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="#000000"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="410dp"
        android:layout_height="115dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />


    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4">

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <Button
                android:id="@+id/btAthelstan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:background="@drawable/athelstan"
                android:gravity="center"
                android:text="Athelstan"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="151dp" />

            <Button
                android:id="@+id/btBjorn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bjorn"
                android:gravity="center"
                android:text="Bjorn"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="151dp" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <Button
                android:id="@+id/btFloki"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/floki"
                android:gravity="center"
                android:text="Floki"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="343dp" />

            <Button
                android:id="@+id/btLagertha"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/lagertha"
                android:gravity="center"
                android:text="Largertha"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="351dp" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <Button
                android:id="@+id/btRagnar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/ragnar"
                android:gravity="center"
                android:text="Ragnar"
                tools:layout_editor_absoluteX="36dp"
                tools:layout_editor_absoluteY="531dp" />

            <Button
                android:id="@+id/btRollo"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/rollo"
                android:gravity="center"
                android:text="Rollo"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="531dp" />
        </TableRow>

    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>